    "@react-navigation/bottom-tabs": "^7.8.12",
    "@react-navigation/native": "^7.1.25",
    "@react-navigation/stack": "^7.6.12",
    "@supabase/supabase-js": "^2.87.1",
    "babel-preset-expo": "~54.0.0",
    "expo": "~54.0.29",
    "expo-camera": "~17.0.10",
    "expo-dev-client": "~6.0.20",
    "expo-file-system": "^19.0.20",
    "expo-font": "~14.0.10",
