{
  "name": "planta-app",
  "version": "1.0.0",
  "description": "Educultivo - Aplicativo para cuidar das suas plantas, acompanhar crescimento e conectar com uma comunidade de jardineiros.",
  "main": "index.js",
  "author": "Educultivo Team",
  "license": "MIT",
  "keywords": [
    "plantas",
    "jardinagem",
    "react-native",
    "expo",
    "cuidados",
    "natureza"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/planta-team/planta-app.git"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:dev": "npx eas build --profile development --platform android",
    "build:preview": "npx eas build --profile preview --platform android",
    "build:production": "npx eas build --profile production --platform android",
    "build:ios": "npx eas build --platform ios --profile production",
    "build:android": "npx eas build --platform android --profile production",
    "submit:ios": "npx eas submit --platform ios",
    "submit:android": "npx eas submit --platform android"
  },
  "dependencies": {
    "@expo-google-fonts/overlock": "^0.4.1",
